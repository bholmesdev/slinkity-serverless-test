    "sass": "^1.50.0",
    "svelte": "^3.46.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.9",
    "@slinkity/renderer-svelte": "^1.0.0",
    "slinkity": "file:slinkity-0.8.2.tgz"
  }
}
