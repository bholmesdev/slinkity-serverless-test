{
  "name": "my-slinkity-site",
  "version": "1.0.0",
  "description": "A splendid starter for my Slinkity site!",
  "main": ".eleventy.js",
  "os": [
    "linux",
    "darwin",
    "windows"
  ],
  "scripts": {
    "dev": "eleventy --serve --incremental",
